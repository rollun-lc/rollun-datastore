{
  "name": "rollun-com/rollun-datastore",
  "description": "Datastore",
  "minimum-stability": "stable",
  "license": "proprietary",
  "type": "library",
  "authors": [
    {
      "name": "avz-cmf",
      "email": "email@example.com"
    },
    {
      "name": "victorynox",
      "email": "it.proffesor02@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-pdo": "*",
    "ext-json": "*",
    "xiag/rql-parser": "^1.0",
    "symfony/filesystem": "^3.0",
    "rollun-com/rollun-utils": "^4.0||^5.0",
    "rollun-com/rollun-logger": "^4.0||^5.0||^6.0",
    "rollun-com/rollun-installer": "^4.0",
    "zendframework/zend-http": "^2.0",
    "zendframework/zend-servicemanager": "^3.0",
    "zendframework/zend-db": "^2.9",
    "zendframework/zend-diactoros": "^2.0",
    "zendframework/zend-stratigility": "^3.0",
    "ajgl/csv-rfc": "^0.3.0",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "zendframework/zend-expressive": "^3.2.1",
    "zendframework/zend-component-installer": "^2.0",
    "zendframework/zend-config-aggregator": "^1.0",
    "phpunit/phpunit": "^7.0",
    "zfcampus/zf-development-mode": "^3.1",
    "filp/whoops": "^2.1.7",
    "symfony/dotenv": "^4.1",
    "squizlabs/php_codesniffer": "^3.0",
    "zendframework/zend-httphandlerrunner": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "rollun\\datastore\\": "src/DataStore/src",
      "rollun\\files\\": "src/Files/src",
      "rollun\\uploader\\": "src/Uploader/src",
      "rollun\\repository\\": "src/Repository/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "rollun\\test\\": "test"
    }
  },
  "extra": {
    "zf": {
      "config-provider": [
        "rollun\\datastore\\ConfigProvider",
        "rollun\\uploader\\ConfigProvider",
        "rollun\\repository\\ConfigProvider"
      ]
    }
  },
  "scripts": {
    "test": "phpunit --colors=always",
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
    "upload-coverage": "coveralls -v",
    "code-sniffer": "phpcs .",
    "code-beautiful": "phpcbf .",
    "server": "php -S 0.0.0.0:8000 -t public public/index.php"
  }
}
